--langdef=imywasql
--langmap=imywasql:.sql
--regex-imywasql=/^create procedure *([a-zA-Z0-9_]+)/\1/d,procedure/
--regex-imywasql=/^create table (if not exists)? ([a-zA-Z0-9_]+)/\2/d,table/

--langdef=widgetjs
--langmap=widgetjs:.wdjs
--regex-widgetjs=/^\t([a-zA-Z0-9_]+): function/\1/d,procedure/
--regex-widgetjs=/^\t(options):/\1/d,variable/
